#version 450 core

layout (local_size_x=256) in;

layout(set=0,binding=0) buffer  inDat{
    int dat[];
} inInt;

layout(set=0,binding=1) buffer outDat{
    int dat[ ];
}outInt;

void main(){
    uint glID = gl_GlobalInvocationID.x;
    if(glID < 64){
        outInt.dat[glID] = inInt.dat[glID]+100;
        inInt.dat[glID]=outInt.dat[glID]+100;
    }

}